#include<stdio.h>
int main(){
    int start,end,i;

        printf("enter start value:");
        scanf("%d",&start);

        printf("enter end value:");
        scanf("%d",&end);

        printf("odd numbers:");
        for(i=start;i<=end;i++){
            if (i%2 !=0){
                printf("%d",i);
            }
        }

        printf("even numbers:");
        for(i=start;i<=end; i++){
            if (i%2==0){
                printf("%d",i);
            }
        }
}
